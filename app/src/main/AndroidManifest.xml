<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luciana.techpain">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:name=".AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Cadastro" />
        <!-- <activity android:name="com.luciana.techpain.Body1" /> NO XML REFERENCE -->
        <activity android:name=".BodyImage" />
        <activity
            android:name=".PerfilAdicao"
            android:label="Paciente" />
        <activity android:name=".Ampere" />
        <activity
            android:name=".SplashScreen"
            android:label="TechPain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Intensity"
            android:label="@string/title_activity_intensity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>